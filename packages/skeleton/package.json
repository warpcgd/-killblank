{
  "name": "@killblank/skeleton",
  "version": "1.0.1",
  "description": "骨架屏核心代码",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/typings/index.d.ts",
  "types": "./dist/typings/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "watch": "tsc -w -p ./tsconfig.json",
    "dev": "cross-env TS_NODE_PROJECT=\"../../tsconfig.webpack.json\" webpack --config ./webpack.config.ts --mode=development --watch ",
    "build:umd": "cross-env TS_NODE_PROJECT=\"../../tsconfig.webpack.json\" webpack --config ./webpack.config.ts",
    "build:esm": "tsc -p ./tsconfig.json",
    "build:typings": "tsc -p ./tsconfig.typings.json",
    "build": "npm run build:umd && npm run build:esm && npm run build:typings"
  },
  "dependencies": {
    "hashids": "^2.2.2",
    "js-base64": "^3.5.2",
    "lodash": "^4.17.20",
    "rgbaster": "^2.1.1",
    "typescript": "^3.9.7"
  },
  "author": "lixichen<warpcgd@qq.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "typescript": "^3.9.7"
  }
}
